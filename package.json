{
	"name": "pict-section-objecteditor",
	"version": "1.0.0",
	"description": "Pict tree-based JSON object editor section",
	"main": "source/Pict-Section-ObjectEditor.js",
	"scripts": {
		"start": "node source/Pict-Section-ObjectEditor.js",
		"test": "npx mocha -u tdd -R spec",
		"tests": "npx mocha -u tdd --exit -R spec --grep",
		"coverage": "npx nyc --reporter=lcov --reporter=text-lcov npx mocha -- -u tdd -R spec",
		"build": "npx quack build"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/stevenvelozo/pict-section-objecteditor.git"
	},
	"author": "steven velozo <steven@velozo.com>",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/stevenvelozo/pict-section-objecteditor/issues"
	},
	"homepage": "https://github.com/stevenvelozo/pict-section-objecteditor#readme",
	"dependencies": {
		"pict-view": "^1.0.66"
	},
	"devDependencies": {
		"pict": "^1.0.348",
		"quackage": "^1.0.51"
	},
	"mocha": {
		"diff": true,
		"extension": ["js"],
		"package": "./package.json",
		"reporter": "spec",
		"slow": "75",
		"timeout": "5000",
		"ui": "tdd",
		"watch-files": [
			"source/**/*.js",
			"test/**/*.js"
		],
		"watch-ignore": [
			"lib/vendor"
		]
	}
}
